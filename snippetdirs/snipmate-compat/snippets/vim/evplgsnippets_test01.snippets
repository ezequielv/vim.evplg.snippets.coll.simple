
snippet testmulti1 test: multi-line expansion (#1)
	before first line: `join(['line1','line2','line3'], "\\n")` <- after last line

snippet testmulti2 test: multi-line expansion (#2)
	before ${1:first} line: `join(['line1','line2','line3'], "\\n")` <- after ${2:last} line

snippet testmulti3 test: multi-line expansion (#3)
	before ${1:first} line: `join(['line1','line2 ${2:field2}','line3'], "\\n")` <- after ${3:last} line

snippet testmulti4 test: multi-line expansion (#4)
	before ${1:first} line: `join(['line1','line2','line3'], "\\n")` <- after last line
	second field: ${2:second}

